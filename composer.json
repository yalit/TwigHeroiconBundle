{
  "name": "yalit/twig-heroicon-bundle",
  "description": "A simple bundle to load Tailwind Heroicon icons from Twig template",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8",
    "twig/twig": "^2.12|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Yalit\\TwigHeroiconBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yalit\\TwigHeroiconBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  }
}
